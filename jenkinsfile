pipeline {
    agent any
    stages{
        stage('checkout') {
            steps {
                git url: 'https://github.com/akashsakore/test.git'
            }
        }
        stage('build') {
            steps{
                sh 'mvn clean package'
            }
        }
        stage('test') {
            steps{
                sh 'mvn test'
            }
        }
    }
}